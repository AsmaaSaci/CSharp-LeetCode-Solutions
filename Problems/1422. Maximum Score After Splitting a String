public class Solution {
    public int MaxScore(string s) {
        int maxScore = 0;
        int ones = 0;
        int zeroes = 0;
        for(int i = 0; i < s.Length; i++)
        {
            if(s[i] == '1') ones++;
        }

        for(int i = 0; i < s.Length - 1; i++)
        {
            if(s[i] == '1') 
            {
                ones--;
            }
            else
            {
                zeroes++;
            }
            if(zeroes + ones > maxScore) maxScore = zeroes + ones; 
        }

        return maxScore;
    }
}
