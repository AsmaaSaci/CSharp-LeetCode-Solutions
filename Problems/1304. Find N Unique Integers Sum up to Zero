public class Solution {
    public int[] SumZero(int n) {
        int[] integers = new int[n];

        for (int i = 0; i < n / 2; i++) {
            integers[2 * i] = i + 1;
            integers[2 * i + 1] = -(i + 1);
        }

        if (n % 2 == 1) {
            integers[n - 1] = 0;
        }

        return integers;
    }
}
