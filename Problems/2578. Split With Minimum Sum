public class Solution {
    public int SplitNum(int num) {
        int[] freq = new int[10];
        while (num > 0) {
            freq[num % 10]++;
            num /= 10;
        }
        int n1 = 0, n2 = 0;
        bool odd = true;
        for (int i = 0; i < 10; i++) {
            while (freq[i] > 0) {
                if (odd) {
                    n1 = n1 * 10 + i;
                } else {
                    n2 = n2 * 10 + i;
                }
                freq[i]--;
                odd = !odd;
            }
        }
        return n1 + n2;
    }
}
