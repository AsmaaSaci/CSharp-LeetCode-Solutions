public class Solution {
    public int SumOfEncryptedInt(int[] nums) {
        int temp = 0, sum = 0;
        for(int i = 0; i < nums.Length; i++){
            int mul = 0;
            while(nums[i] > 0){
                if(temp < nums[i] % 10)
                    temp = nums[i] % 10;
                mul = mul * 10 + 1;
                nums[i] = nums[i] / 10;
            }
            sum += temp * mul;
            temp = 0;
        }
        return sum;
    }
}
