public class Solution {
    public int WinningPlayerCount(int n, int[][] pick) 
    {
        int winners = 0;
        int[][] score = new int[n][];

        for(int i = 0; i < n; i++)
        {
            score[i] = new int[11];
        }

        foreach(var p in pick)
        {
            score[p[0]][p[1]]++;
        }

        for(int i = 0; i < n; i++)
        {
            for(int j = 0; j <= 10; j++)
            {
                if(score[i][j] > i)
                {
                    winners++;
                    break;
                }
            }
        }   
        return  winners;
    }
}
