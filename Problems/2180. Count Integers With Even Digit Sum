public class Solution {
    public int CountEven(int num) {
        int count = 0;

        for (int i = 1; i <= num; i++) {
            int sum = 0;
            int temp = i;

            // Calculate digit sum
            while (temp > 0) {
                sum += temp % 10;
                temp /= 10;
            }

            // Check even digit sum
            if (sum % 2 == 0) {
                count++;
            }
        }
        return count;
    }
}
