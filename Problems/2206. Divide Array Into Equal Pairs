public class Solution {
    public bool DivideArray(int[] nums) {
        
        Dictionary<int,int> num = new Dictionary<int,int>();

        foreach (int n in nums)
        {
            if (num.ContainsKey(n))
            {
                num[n]++;
            }
            else
            {
                num[n] = 1;
            }
        }

        foreach (var val in num.Values)
        {
            if (val % 2 != 0)
            {
                return false;
            }
        }

        return true;
    }
}
