public class Solution {
    public int MaxLengthBetweenEqualCharacters(string s) {
        int max = -1;
        for (int i = 0; i < s.Length; i++)
        {
            for (int j = s.Length - 1; j >= 0; j--)
            {
                if (s[i] == s[j])
                {
                    int lon = j - i - 1;
                    if(lon > max) max = lon;
                    break;
                }
            }
        }
        return max;
    }
}
