public class Solution {
    public char RepeatedCharacter(string s) {
        HashSet<char> seen = new HashSet<char>();

        for(int i = 0; i < s.Length; i++) {
            char letter = s[i];
            if(seen.Contains(letter)) {
                return letter;
            }
            seen.Add(letter);
        }

        return 'a';
    }
}
