public class Solution {
    public bool IsArraySpecial(int[] nums) {
        bool isSpecial=true;
        if(nums.Length>1)
        {
        for(int i=0;i<=nums.Length;i++)
        {
            if(i<nums.Length-1){
            if((nums[i]%2) == (nums[i+1]%2))
            {
                isSpecial= false;
                if(!isSpecial)
                return isSpecial;
            }            }
        }
        }        
        return isSpecial;
    }
}
