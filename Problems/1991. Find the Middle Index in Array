public class Solution {
    public int FindMiddleIndex(int[] nums) {
        int numsL = nums.Length;
        int index = -1;

        int sumAll = 0;
        foreach (int num in nums) { sumAll += num; }

        int sumL = 0;
        for (int i = 0; i < numsL; i++)
        {
            if (i > 0) { sumL += nums[i - 1]; }
            if (sumL == sumAll - sumL - nums[i])
            {
                index = i;
                break;
            }
        }

        return index;
    }
}
