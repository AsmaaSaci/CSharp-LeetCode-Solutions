public class Solution {
    public int FindNonMinOrMax(int[] nums) {
        if(nums.Length <= 2) {
            return -1;
        }

        int minNumber = nums[0];
        int maxNumber = nums[0];

        // Find minimum and maximum numbers in nums
        foreach(int num in nums) {
            if(num < minNumber) {
                minNumber = num;
            } else if(num > maxNumber) {
                maxNumber = num;
            }
        }

        // Return the first number that isn't the min or max
        foreach(int num in nums) {
            if(num != minNumber && num != maxNumber) {
                return num;
            }
        }

        return nums[0];
    }
}
