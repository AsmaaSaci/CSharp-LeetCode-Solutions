public class Solution {
    public int FindGCD(int[] nums) {
        Array.Sort(nums);
        int low = nums[0];
        int high = nums[nums.Length - 1];
        
        return gcd(high, low); 
    }
    public int gcd(int p, int q)
    {
        if(q == 0) return p;
        int r = p % q;
        return gcd(q, r);
    }
}
