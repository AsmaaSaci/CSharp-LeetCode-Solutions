public class Solution {
    public int GcdOfOddEvenSums(int n) {
        int sumodd=0;int sumeven=0;
        for(int i=1;i<=n*2;i++)
        {
            if(i%2!=0)
            {
            sumodd+=i;
            }
            else
            {
                sumeven+=i;
            }
            
        }
        int cd(int a,int b)
        {
            if(b==0)
            {
                return a;
            }
            return cd(b,a%b);
        }
        return cd(sumodd,sumeven);
    }
}
