public class Solution {
    public int GetMaximumGenerated(int n) {
        int[] res = new int[n + 1];
        int j = 1;
        for(int i = 0; i <= n; i++)
        {
            if(i == 0)
            {
                res[i] = 0;
            }
            else if(i == 1)
            {
                res[i] = 1;
            }
            else if(i > 1)
            {
                if(i % 2 == 0)
                {
                    res[i] = res[i - j]; 
                }
                else
                {
                    res[i] = res[j] + res[j + 1];
                    j++;
                }
            }
        }
        return res.Max();
    }
}
