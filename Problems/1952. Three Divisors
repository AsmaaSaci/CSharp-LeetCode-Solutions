public class Solution {
    public bool IsThree(int n) {
        
        // Raj Thomas 18th Aug 2025 1:15AM IST

        // main case
        int count = 2; // 0 and N are divisors itself
        for(int i = 2; i < n; i++) {
            if(n % i == 0) {
                count++;
            }

            if(count > 3) return false;
        }

        return count == 3 ? true : false;
    }
}
