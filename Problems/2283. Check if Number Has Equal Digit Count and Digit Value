public class Solution 
{
    public bool DigitCount(string num)
    {
        int[] digits = new int[10];

        for (int i = 0; i < num.Length; i++)
        {
            digits[num[i] - '0']++;
        }

        for (int i = 0; i < num.Length; i++)
        {
            if (digits[i] != num[i] - '0')
            {
                return false;
            }
        }

        return true;
    }
}
