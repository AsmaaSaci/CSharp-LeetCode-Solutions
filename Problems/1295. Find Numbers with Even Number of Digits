public class Solution {
    public int FindNumbers(int[] nums) {
        
        int number = 0;
        int counter =0;
        int evenDigit = 0;
        for(int i =0 ;  i < nums.Length ; i++)
        {
            number = nums[i];
            while(number>0)
            {
                number = number / 10;
                counter++;
              }
            
                if(counter%2 == 0)
                {
                    evenDigit ++;
                }
            counter = 0;  
        }
        return evenDigit;
    }
}
