public class Solution {
    public int CountLargestGroup(int n) {
        
        // Raj Thomas 28th aug 2025 5:53PM IST

        Dictionary<int, int> groupFreq = new();
        int i = 1;
        while(i <= n) {
            
            int num = i;
            int sum = 0;
            while(num > 0) {
                sum += num % 10;
                num = num / 10;
            }

            groupFreq[sum] = groupFreq.GetValueOrDefault(sum, 0) + 1;
            i++;
        }


        int max = groupFreq.Max(kvp => kvp.Value);        
        return groupFreq.Where(kvp => kvp.Value == max).Count();
    }
}
