public class Solution {
    public string[] FindRelativeRanks(int[] score) {
        int scoreL = score.Length;
        Dictionary<int,int> ranking = new Dictionary<int,int>();
        string[] ketQua = new string[scoreL];

        for (int i = 0; i < scoreL; i++)
        {
            ketQua[i] = score[i].ToString();
            ranking.Add(score[i], i);
        }
        var sortedRank = ranking.OrderByDescending(x => x.Key).ToList();

        string rankStr = "";
        int count = 0;

        foreach(var kvp in sortedRank)
        {
            count++;
            if (count == 1) rankStr = "Gold Medal";
            if (count == 2) rankStr = "Silver Medal";
            if (count == 3) rankStr = "Bronze Medal";
            if (count > 3) rankStr = count.ToString();

            ketQua[kvp.Value] = rankStr;
        }

        return ketQua;
    }
}
