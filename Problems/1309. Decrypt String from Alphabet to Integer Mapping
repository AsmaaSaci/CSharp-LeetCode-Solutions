public class Solution {
public string FreqAlphabets(string s)
{
    StringBuilder sb = new();
    int i = 0;
    while(i < s.Length)
    {
        if (i < s.Length - 2 && s[i + 2] == '#')
        {
            sb.Append((char)('j' + (s[i] - '1') * 10 + s[i + 1] - '0'));
            i += 3;
        }
        else
        {
            sb.Append((char)('a' + (s[i] - '1')));
            i++;
        }
    }
    return sb.ToString();
}
}
