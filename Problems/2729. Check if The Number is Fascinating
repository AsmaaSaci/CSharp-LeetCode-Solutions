public class Solution {
    public bool IsFascinating(int n)
    {
        StringBuilder sb = new StringBuilder();
        sb.Append(n).Append(n*2).Append(n*3);

        if (!sb.ToString().Length.Equals(9))
            return false;

        for (int i = 1; i <= 9; i++)
            if (!sb.ToString().Contains($"{i}"))
                return false;
                
        return true;
    }
}
