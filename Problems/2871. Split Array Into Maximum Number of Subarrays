public class Solution {
    public int MaxSubarrays(int[] nums) {
        int res = 0, pre = -1; // -1 -> 32 1s in binary
        foreach(int n in nums)
        {
            pre &= n;
            if(pre == 0) // & result to be 0? start another subArray
            {
                res++;
                pre = -1;
            }
        }

        return Math.Max(1, res);
    }
}
