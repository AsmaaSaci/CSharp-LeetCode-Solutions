public class Solution {
    public int MinOperations(int[] nums, int k) {
        var greater = new HashSet<int>();
        for(int i = 0; i<nums.Length; i++){
            var num = nums[i];
            if(num < k)
                return -1;
            
            if(num > k)
                greater.Add(num);
        }

        return greater.Count;
    }
}
