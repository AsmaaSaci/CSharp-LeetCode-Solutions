public class Solution {
    public int LargestInteger(int[] nums, int k) {
        int n = nums.Length;
        var subarrayCount = new Dictionary<int, int>();
        
        // Process all subarrays of size k
        for (int i = 0; i <= n - k; i++) {
            var uniqueNums = new HashSet<int>();
            for (int j = i; j < i + k; j++) {
                uniqueNums.Add(nums[j]);
            }
            foreach (int num in uniqueNums) {
                if (!subarrayCount.ContainsKey(num)) {
                    subarrayCount[num] = 0;
                }
                subarrayCount[num]++;
            }
        }
        
        // Find the largest integer that appears in exactly one subarray
        int ans = -1;
        foreach (var entry in subarrayCount) {
            if (entry.Value == 1 && entry.Key > ans) {
                ans = entry.Key;
            }
        }
        
        return ans;
    }
}
