public class Solution {
    public int CountCompleteDayPairs(int[] hours) {


        // Raj Thomas 16th July 2025 2:38PM IST

        // base case
        int counter = 0;
        Hashtable ht = new Hashtable();

        for(int i =0; i < hours.Length; i++) {

            // both remainder are complement are required
            int remainder = hours[i] % 24;

            // This is for 0 sepcial case
            int complement = (24 - remainder) % 24;

            if(ht.ContainsKey(complement)) {
                counter += (int)ht[complement];
            }

            if(ht.ContainsKey(remainder)) {
                ht[remainder] = (int)ht[remainder] + 1;
            }
            else {
                ht.Add(remainder, 1);
            }

        }
        return counter;

    }
}
