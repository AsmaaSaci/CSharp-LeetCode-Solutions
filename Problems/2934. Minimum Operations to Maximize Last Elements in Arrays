public class Solution {
    public int MinOperations(int[] nums1, int[] nums2) {
        int max1 = nums1[nums1.Length - 1];
        int max2 = nums2[nums2.Length - 1];
        int count = 0;
        int count1 = 1;
        for(int i = 0; i < nums1.Length - 1; i++)
        {
            if(nums1[i] <= max1 && nums2[i] <= max2)
            {
                continue;
            }
            if(nums1[i] <= max2 && nums2[i] <= max1)
            {
                count++;
                continue;
            }
            return -1;
        }
        for(int i = 0; i < nums1.Length - 1; i++)
        {
            if (nums1[i] <= max2 && nums2[i] <= max1)
            {
                continue;
            }
            if (nums1[i] <= max1 && nums2[i] <= max2)
            {
                count1++;
                continue;
            }
            return -1;
        }
        return Math.Min(count, count1);
    }
}
