public class Solution {
    public int PartitionString(string s) {
        HashSet<char> seen = new HashSet<char>();
        int partitions = 1;

        foreach (char c in s) {
            if (seen.Contains(c)) {
                partitions++;
                seen.Clear();
            }
            seen.Add(c);
        }
        return partitions;
    }
}
