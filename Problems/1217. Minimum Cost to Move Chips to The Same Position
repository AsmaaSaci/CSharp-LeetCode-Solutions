public class Solution {
    public int MinCostToMoveChips(int[] position) {
        var count1=0;
        var count2=0;
        for(int i=0;i<position.Length;i++)
            if(position[i]%2==0) 
                count1++;
            else count2++;
        return count1>count2?count2:count1;
    }
}
