public class Solution {
    public string LastNonEmptyString(string s) {
        int[] freq = new int[26];
        int maxF = 1;
        foreach(char c in s)
        {
            freq[c-'a']++;
            maxF = Math.Max(maxF, freq[c-'a']);
        }

        string res = "";
        for(int i = s.Length-1; i >= 0; i--)
        {
            char c = s[i];
            if(freq[c-'a'] == maxF)
            {
                freq[c-'a'] = -1;
                
                 res = c+res;
            }  
        }

        return res;
    }
}
