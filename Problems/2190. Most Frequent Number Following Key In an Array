public class Solution {
    public int MostFrequent(int[] nums, int key) {
        var freq = new Dictionary<int,int>();
        int n = nums.Length;
        int target = 0;


     for(int i =0; i < n-1;i++){
        if(nums[i] == key){
            target = nums[i+1];

            if(!freq.ContainsKey(target)) freq[target] = 0;
            freq[target]++;
        }
     }

     int res = -1;
     int maxVal = 0;

     foreach(var kvp in freq){
        if(kvp.Value > maxVal){
            maxVal = kvp.Value;
            res = kvp.Key;
        }
     }

   return res;

    }
}
