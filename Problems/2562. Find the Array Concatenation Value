public class Solution {
    public long FindTheArrayConcVal(int[] nums) {
        int start=0;
        int end=nums.Length-1;
        long result=0;
        while (start <= end){
            if(start == end){
                result+=nums[start];
                start+=1;
            }
            else{
                int checkEnd=1;
                while(checkEnd <= nums[end]){
                    checkEnd*=10;
                }
                result+=(long)nums[start]*checkEnd+nums[end];
                start+=1;
                end-=1;
            }
        }
        return result;
    }
}
