public class Solution {
    public string MakeGood(string s) 
    {
        StringBuilder sb = new();
        foreach(char c in s)
        {
            if (sb.Length > 0 && Math.Abs(sb[sb.Length - 1] - c) == 32) sb.Length--; // remove previous char (cancel out)
            else sb.Append(c);
        }
        return sb.ToString();
    }
}
