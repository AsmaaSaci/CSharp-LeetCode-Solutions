//C# Code
public class Solution {
    public int SumCounts(IList<int> nums) {
        int n = nums.Count();
        int res = 0;
        for(int i = 0; i < n; i++){
            HashSet<int> h = new HashSet<int>();
            for(int j = i; j < n; ++j){
                h.Add(nums[j]);
                res += (int)Math.Pow(h.Count(), 2);
            }
        }
        return res;
    }
}
