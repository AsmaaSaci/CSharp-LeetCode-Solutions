public class Solution {
    public int DifferenceOfSum(int[] nums) {
        int sum1=0,sum2=0;
        for(int i=0;i<nums.Length;i++){
          sum1+=nums[i]; //Sum of all elements.

          while(nums[i]!=0){
              sum2+=nums[i]%10;  //sum of all digits separately.l
              nums[i]/=10;
          }
        }
        return Math.Abs(sum1-sum2);
    }
}
