public class Solution {
    public int Maximum69Number (int num) {
        char[] s=num.ToString().ToCharArray();
        int maxV=num;
        for(int i=0;i<s.Length;i++){
            s[i]= s[i]=='6'? '9':'6';
            int temp=Convert.ToInt32(new String(s));
            if(maxV<temp)
                maxV=temp;
            s[i]= s[i]=='6'? '9':'6';
        }
        return maxV;
    }
}
