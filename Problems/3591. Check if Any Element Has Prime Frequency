public class Solution {
    public bool CheckPrimeFrequency(int[] nums) {
        int counter = 0;
        var dic = new Dictionary<int, int>();

        foreach (var num in nums)
        {
            if (dic.ContainsKey(num))
                dic[num]++;
            else
                dic[num] = 1;
        }

        return dic.Any(d => IsPrime(d.Value));
        bool IsPrime(int n)
        {
            counter = 0;

            if (n is 1)
                return false;
            else if (n is 2)
                return true;
            else if (n % 2 == 0)
                return false;
            else
            {
                for (int i = 1; i <= n; i+=2)
                {
                    if (n % i == 0)
                        counter++;
                }

                return counter <= 2;
            }
        }
    }
}
