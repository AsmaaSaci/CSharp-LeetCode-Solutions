public class Solution {
    public int SmallestNumber(int n, int t) {
        while (true) {
            int product = 1;
            string digits = n.ToString();
            foreach (char ch in digits) {
                int digit = ch - '0';
                product *= digit;
            }
            if (product % t == 0) {
                return n;
            }
            n++;
        }
    }
}
